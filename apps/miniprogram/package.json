{
  "name": "@funnymath/miniprogram",
  "version": "0.1.0",
  "private": true,
  "description": "FunnyMath WeChat Mini Program built with Taro",
  "scripts": {
    "dev:weapp": "npm run build:weapp -- --watch",
    "build:weapp": "taro build --type weapp",
    "dev:swan": "npm run build:swan -- --watch",
    "build:swan": "taro build --type swan",
    "dev:alipay": "npm run build:alipay -- --watch",
    "build:alipay": "taro build --type alipay",
    "dev:tt": "npm run build:tt -- --watch",
    "build:tt": "taro build --type tt",
    "dev:h5": "npm run build:h5 -- --watch",
    "build:h5": "taro build --type h5",
    "dev:rn": "npm run build:rn -- --watch",
    "build:rn": "taro build --type rn",
    "dev:qq": "npm run build:qq -- --watch",
    "build:qq": "taro build --type qq",
    "dev:jd": "npm run build:jd -- --watch",
    "build:jd": "taro build --type jd"
  },
  "dependencies": {
    "@funnymath/content": "workspace:^",
    "@funnymath/ui": "workspace:^",
    "@tarojs/components": "3.6.24",
    "@tarojs/helper": "3.6.24",
    "@tarojs/plugin-platform-weapp": "3.6.24",
    "@tarojs/plugin-framework-react": "3.6.24",
    "@tarojs/runtime": "3.6.24",
    "@tarojs/taro": "3.6.24",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@tarojs/cli": "3.6.24",
    "@tarojs/webpack5-runner": "3.6.24",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "eslint-config-taro": "3.6.24",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "stylelint": "^15.0.0",
    "typescript": "^5.0.0",
    "webpack": "5.78.0"
  }
}
